{
  "manifest_version": 2,

  "name": "Clicky",
  "description": "Post links to Slack with a single click!",
  "version": "1.0.0",
  "permissions": [
    "https://slack.com/api/*",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/icon32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}