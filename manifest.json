{
  "manifest_version": 2,

  "name": "#Clicky for Slack",
  "short_name": "#Clicky",
  "description": "Post links to Slack with a single click!",
  "version": "2.2.8",

  "key": "",

  "permissions": [
    "*://*/*",
    "tabs",
    "notifications",
    "identity",
    "contextMenus"
  ],
  "content_security_policy": "script-src 'self' https://js.intercomcdn.com https://widget.intercom.io https://stats.g.doubleclick.net; object-src 'self'",

  "background": {
    "scripts": [
    "/components/jquery/dist/jquery.js",
    "/js/background.js"
    ]
  },

  "oauth2": {
    "client_id": "583354279633-l4nsfk7q1mucdjirpt9lincrsqhigm3j.apps.googleusercontent.com",
    "scopes": [
      "https://slack.com/oauth/authorize"
    ]
  },

  "browser_action": {
    "default_icon": "assets/icon32.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
